mixin sBrends(data)
	// start sBrends
	+b.SECTION.sBrends.section(class=data.addClasses)#sBrends&attributes(attributes)
		.container(class=data.addContClasses)
			+b.section-title(class=data.addSectinTitleClasses)
				h2=data.title
			+e.row.row
				-for(var i = 1; i <= 12; i++)
					+sBrendsItem(i)
	// end sBrends

mixin sBrendsItem(imgNum)
	.col-6.col-sm-4.col-md-3.col-xl-2
		+e.item(href='#')
			img(loading="lazy" src=`img/@2x/sBrends-img-${imgNum}.jpg` alt="")
